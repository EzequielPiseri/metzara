Package: metzara
Title: Paquete De Funciones Metereologicos Para El Segundo Parcial
Version: 0.0.0.9000
Authors@R: 
     c(person(given = "Ezequiel", family = "Piseri", email = "ezequielpiseri@gmail.com", role = c("aut","cre")),
